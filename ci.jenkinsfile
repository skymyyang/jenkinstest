#!groovy


String buildShell = "${env.buildShell}"
pipeline{
  agent { node { label "build-01"}}
  
  stages{
    stage("build"){
      steps{
      
           script{
              mvnHome = tool "M2"
             sh "${mvnHome}/bin/mvn ${buildShell}"     
         }
      }     
   }
    stage("antbuild"){
      steps{
        script{
          antHome = tool "ANT"
          sh "${antHome}/bin/ant -v"
        }
      }
    }
  }
}
